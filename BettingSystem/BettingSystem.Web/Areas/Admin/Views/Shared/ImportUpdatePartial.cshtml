<form method="post" enctype="multipart/form-data" asp-controller="Imports" asp-action="ImportUpdateEvents">
    <div class="form-group">
        <div class="col-12">
            <p>Upload one or more files using this form:</p>
            <input type="file" name="@nameof(UpdateEventsImportViewModel.XmlFile)" class="form-control-file" multiple/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-12">
            <button type="submit" class="btn btn-primary btn-block">Update</button>
        </div>
    </div>
</form>